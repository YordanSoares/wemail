{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "gettext": {
      "plugins": [
        ["./assets/src/js/i18n/babel-plugin", {
          "output": "i18n/languages/wemail.pot"
        }]
      ]
    }
  }
}
